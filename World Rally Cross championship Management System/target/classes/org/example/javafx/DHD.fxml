<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="background" fx:id="anchorpaneDHD" prefHeight="315.0" prefWidth="501.0" stylesheets="@DHD.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.javafx.DHD">
    <children>
        <AnchorPane id="head" layoutY="-1.0" prefHeight="50.0" prefWidth="501.0">
            <children>
                <Label layoutX="154.0" layoutY="13.0" text="Delete Horse Details" textFill="WHITE">
                    <font>
                        <Font name="Britannic Bold" size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <TextField id="text-field" fx:id="fieldhorseIDfromDHD" alignment="CENTER" layoutX="207.0" layoutY="123.0" onAction="#horseIDfromDHD" promptText="Horse ID">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <Label layoutX="72.0" layoutY="127.0" prefHeight="17.0" prefWidth="68.0" text="Horse ID :">
            <font>
                <Font name="Bell MT Bold" size="14.0" />
            </font>
        </Label>
      <Button id="button" fx:id="btnbackDHD" layoutX="65.0" layoutY="243.0" mnemonicParsing="false" onAction="#backformDHD" prefHeight="28.0" prefWidth="68.0" text="Back" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Button>
      <Button id="button" fx:id="btndeleted_horse" layoutX="401.0" layoutY="243.0" mnemonicParsing="false" onAction="#deleted_horse" text="Delete" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
